<!--pages/map/map.wxml-->
<view>
  <map
  id="map"
  longitude="118.076449"
  latitude="24.459576"
  scale="14"
  controls="{{controls}}"
  bindcontroltap="controltap"
  markers="{{markers}}"
  bindmarkertap="markertap"
  polyline="{{polyline}}"
  bindregionchange="regionchange"
  show-location
  style="width: 100%; height: 300px;"
></map>
</view>
